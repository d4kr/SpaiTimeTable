{
    "name": "laravie/html",
    "description": "HTML and Form Builders for the Laravel Framework",
    "license": "MIT",
    "homepage": "https://github.com/laravie/html",
    "keywords": ["laravel", "html", "form"],
    "support": {
        "issues": "https://github.com/laravie/html/issues",
        "source": "https://github.com/laravie/html"
    },
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        },
        {
            "name": "Adam Engebretson",
            "email": "adam@laravelcollective.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Collective\\Html\\": "src/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.6.4",
        "illuminate/http": "~5.4.0",
        "illuminate/routing": "~5.4.0",
        "illuminate/session": "~5.4.0",
        "illuminate/support": "~5.4.0"
    },
    "require-dev": {
        "illuminate/database": "~5.4.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.0"
    },
    "replace": {
        "laravelcollective/html": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
